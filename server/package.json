{
  "name": "temen-tani-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
    "test": "mocha __tests__/**/**/*.* --timeout=10000 --exit",
    "cover": "nyc mocha __tests__/**/**/*.* --timeout=10000 --recursive --exit",
    "lint": "eslint ./",
    "lint-fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasimy-as/temen-tani-services.git"
  },
  "keywords": [
    "farming",
    "hacktoberfest",
    "smart-farming",
    "temen-tani"
  ],
  "author": "Temen-Tani (Nadiah, Aufa, Hasimy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hasimy-as/temen-tani-services/issues"
  },
  "homepage": "https://github.com/hasimy-as/temen-tani-services#readme",
  "dependencies": {
    "chai": "^4.3.4",
    "confidence": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "joigoose": "^8.0.2",
    "jsonwebtoken": "^8.5.1",
    "kafka-node": "^5.0.0",
    "mocha": "^9.1.2",
    "mongoose": "^5.13.7",
    "nyc": "^15.1.0",
    "passport": "^0.5.0",
    "passport-http": "^0.3.0",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1",
    "winston": "^2.4.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.13"
  },
  "nyc": {
    "lines": 45,
    "statements": 45,
    "functions": 45,
    "branches": 45,
    "check-coverage": true,
    "reporter": [
      "html",
      "text",
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "test",
      "coverage",
      "src/database",
      "src/api/modules"
    ],
    "all": true,
    "cache": true,
    "report-dir": "./coverage"
  }
}
